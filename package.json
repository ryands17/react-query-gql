{
  "name": "react-query-gql",
  "version": "0.0.0",
  "private": true,
  "description": "React Typescript Starter Template with Vite",
  "license": "MIT",
  "scripts": {
    "dev": "run-p client:dev schema:dev",
    "client:dev": "vite",
    "schema:dev": "yarn schema --watch",
    "prebuild": "yarn schema",
    "build": "tsc && vite build",
    "test": "jest",
    "serve": "vite preview",
    "schema": "graphql-codegen --config codegen.yml",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "analyze": "source-map-explorer 'dist/assets/*.js'"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.3",
    "react-query": "3.31.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/typescript": "^2.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-react-query": "^3.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@vitejs/plugin-react": "^1.0.4",
    "autoprefixer": "^10.3.7",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^5.0.0",
    "graphql": "^16.0.1",
    "husky": "^7.0.2",
    "isomorphic-fetch": "3.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "msw": "0.35.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.3.9",
    "prettier": "2.4.1",
    "react-test-renderer": "^17.0.2",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^2.2.17",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.2",
    "vite": "^2.6.7",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "eslint --cache ./src",
      "prettier --write --ignore-unknown ."
    ]
  }
}
